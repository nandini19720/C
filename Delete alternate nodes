/*
class Node {
    int data;
    Node next;
    
    public Node(int data){
        this.data = data;
    }
}
*/
class Solution {
    
    public void deleteAlternate (Node head){
        Node curr=head;
        if(head==null){
            return ;
        }
        while(curr!=null){
            if(curr==null||curr.next==null){
                return ;
            }
             curr.next=curr.next.next;
             curr=curr.next;
            
        }
        
        
    }
}
