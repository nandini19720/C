class Solution
{
    //Function to count nodes of a linked list.
    public static int getCount(Node head)
    {
        int count=1;
        Node curr=head;
        if(head==null){
            return 0;
            
        }
    
        while(curr.next!=null){
            curr=curr.next;
           count++;
        }
        return count;
        
        
    }
}
