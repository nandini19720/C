class BST {
    // Function to search a node in BST.
    boolean search(Node root, int x) {
        if(root==null)
        return false;
        if(x<root.data){
            return  search(root.left,x);
         
            
        }
        else if(root.data==x)
        return true;
        else{
          return  search(root.right,x);
               
        }
    
    }
}

