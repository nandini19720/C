class Solution
{
    public void countOddEven(int[] arr, int n)
    {
        int count=0;
        int num=0;
        for(int i=0;i<n;i++){
            if(arr[i]%2==0){
                count++;
            }
            else{
                num++;
            }
           
         
            
        }
          System.out.print(num+" "+count);
      System.out.println(); 


        
       
    }
}
