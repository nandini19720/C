class Solution{
    
    public static int count4Divisibiles(int arr[], int n ) 
    { 
        int count=0;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if((arr[i]+arr[j])%4==0){
                    count++;
                }
            }
        }
        return count;
        
    }
    
}
