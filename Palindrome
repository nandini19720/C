class Solution
{
    public String is_palindrome(int n)
    {
      
        String s1=Integer.toString(n);
        String sol="";
       for(int i=s1.length()-1;i>=0;i--){
           sol+=s1.charAt(i);
       }
       if(s1.equals(sol)){
           return "Yes";
       }
       else{
           return "No";
       }
       
        
        
    }
}
