public class Solution {
    public int solve(int[] A) {
        Arrays.sort(A);
    
        int i=0;
        if(A[A.length-1]==0){
            return 1;
        }
        
        for( i=0;i<=A.length-1;i++){
        
           if(A[i]==(A.length-1-i) && A[i+1]>A[i])
                return 1;
            
            
        }
       
        return -1;
}
}
