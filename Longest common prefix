class Solution {
    public int[] longestCommonPrefix(String s1, String s2){
        int [] arr=new int[2];
        for(int i=0;i<s1.length();i++){
            if(s2.contains(s1.substring(0,i))){
                arr[0]=0;
                arr[1]=i-1;
                
            }
            
            
        }
        return arr;
    }
}
