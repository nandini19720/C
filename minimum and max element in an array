class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        long min=a[0];
        long max=a[1];
        for(int i=0;i<n;i++){
            if(a[i]<min){
                min=a[i];
            }
            if(a[i]>max){
                max=a[i];
            }
        }
        return new pair(min, max);
    }
}
